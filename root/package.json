{
    "name": "{%= name %}",
    "version": "{%= version %}",
    "description": "{%= description %}",
    "keywords": [
        "ecosystem:jquery",
        "jquery-plugin",
        "jquery",
        "plugins",
        "{%= name %}"
    ],
    "repository": {
        "type": "git",
        "url": "{%= repository %}"
    },
    "bugs": {
        "url": "{%= bugs %}"
    },
    "author": {
        "name": "{%= author_name %}",
        "email": "{%= author_email %}",
        "url": "{%= author_url %}"
    },
    "homepage": "{%= homepage %}",
    "contributors": [
        {
            "name": "{%= author_name %}",
            "email": "{%= author_email %}",
            "url": "{%= author_url %}"
        }
    ],
    "license": "{%= licenses %}",
    "dependencies": {
        "jquery": "{%= jquery_version %}"
    },
    "devDependencies": {
        "grunt": "^1.0.1",
        "grunt-contrib-concat": "^0.5.1",
        "grunt-contrib-jshint": "^0.11.3",
        "grunt-contrib-uglify": "^0.9.2",
        "grunt-contrib-watch": "^0.6.1",
        "grunt-jscs": "^2.8.0",
        "grunt-karma": "^0.12.0",
        "jscs": "^2.11.0",
        "karma": "^1.7.1",
        "karma-chrome-launcher": "^2.2.0",
        "karma-firefox-launcher": "^0.1.7",
        "karma-phantomjs-launcher": "^0.2.3",
        "karma-qunit": "^0.1.9",
        "phantomjs": "^2.1.7",
        "qunitjs": "^1.23.1"
    },
    "scripts": {
        "jscs": "jscs src",
        "test": "npm run jscs && grunt test"
    }
}
